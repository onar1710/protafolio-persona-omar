{
  "name": "@gladtek/cooper",
  "type": "module",
  "version": "0.0.1",
  "description": "A premium Astro template engineered for the unknown. Built for speed, endurance, and exploration.",
  "engines": {
    "node": ">=20.18.1"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build && pagefind --site dist && npm run copy-sitemap",
    "build:vercel": "cross-env ADAPTER=vercel astro build && pagefind --site dist",
    "build:netlify": "cross-env ADAPTER=netlify astro build && pagefind --site dist",
    "build:cloudflare": "cross-env ADAPTER=cloudflare astro build && pagefind --site dist",
    "preview": "astro preview",
    "deploy": "npm run build:cloudflare && wrangler deploy",
    "preview:worker": "wrangler dev",
    "astro": "astro",
    "copy-sitemap": "copy /Y dist\\client\\sitemap-0.xml dist\\client\\sitemap.xml"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/node": "^9.5.1",
    "@astrojs/react": "^4.4.2",
    "@astrojs/rss": "^4.0.14",
    "@astrojs/sitemap": "^3.0.4",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.7",
    "@vercel/speed-insights": "^1.3.1",
    "astro": "^5.16.6",
    "astro-embed": "^0.9.2",
    "astro-mermaid": "^1.1.0",
    "astro-robots-txt": "^1.0.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.1.1",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.562.0",
    "mermaid": "^11.12.2",
    "motion": "^12.23.26",
    "pagefind": "^1.4.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^4.1.7",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3",
    "wrangler": "^4.56.0"
  },
  "devDependencies": {
    "@astrojs/netlify": "^6.6.3",
    "@astrojs/vercel": "^8.0.4",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "astro-compress": "^2.3.8",
    "cross-env": "^10.1.0"
  },
  "overrides": {
    "vite": "^6.0.0"
  }
}
